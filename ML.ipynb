    "from sklearn.linear_model import LinearRegression\n",
    "import numpy as np\n",
    "\n",
    "apple = np.array([155,156,157])\n",
    "n = len(apple)\n",
    "\n",
    "\n",
    "model = LinearRegression().fit(np.arange(n).reshape((n, 1)), apple)\n",
    "\n",
    "model.predict([[3], [4]])\n",
